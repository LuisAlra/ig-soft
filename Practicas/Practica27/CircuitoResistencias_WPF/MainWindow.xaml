                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-1.017"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.888"/>
                </TransformGroup>
            </Image.RenderTransform>
