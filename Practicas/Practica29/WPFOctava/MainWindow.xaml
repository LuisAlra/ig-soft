        <Button x:Name="btnotasos2" Content="Button" HorizontalAlignment="Left" Height="162" Margin="295,118,0,0" VerticalAlignment="Top" Width="43" Background="#FF080202" Grid.RowSpan="2" Click="btnotasos2_Click"/>
        <Button x:Name="btnota5" Content=" " HorizontalAlignment="Left" Height="235" Margin="445,118,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="65" Background="White" Click="btnota5_Click"/>
        <Button x:Name="btnotasos3" Content="Button" HorizontalAlignment="Left" Height="162" Margin="422,118,0,0" VerticalAlignment="Top" Width="44" Background="#FF080202" Grid.RowSpan="2" Click="btnotasos3_Click"/>
        <Button x:Name="btnotasos4" Content="Button" HorizontalAlignment="Left" Height="162" Margin="488,118,0,0" VerticalAlignment="Top" Width="42" Background="#FF080202" Grid.RowSpan="2" Click="btnotasos4_Click"/>
