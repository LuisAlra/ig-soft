                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.495"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-2.991"/>
                </TransformGroup>
            </Frame.RenderTransform>
        </Frame>
        <Button x:Name="btnota1" Content=" " HorizontalAlignment="Left" Margin="187,118,0,0" VerticalAlignment="Top" Height="235" Width="65" Background="White" Click="btnota1_Click" Grid.RowSpan="2"/>
