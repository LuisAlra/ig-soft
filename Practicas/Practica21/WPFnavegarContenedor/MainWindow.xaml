        <Button x:Name="Btpag2" Content="Pagina 2" HorizontalAlignment="Left" Margin="37,268,0,0" VerticalAlignment="Top" Height="44" Width="87" Click="Btpag2_Click"/>
        <Frame x:Name="Frame1" Content="Frame" Margin="144,13,38,55"/>
        <Button x:Name="Btpag3" Content="Pagina 3" HorizontalAlignment="Left" Margin="37,325,0,0" VerticalAlignment="Top" Height="44" Width="87" Click="Btpag3_Click"/>
        <Button x:Name="BtFirma" Content="Firma" HorizontalAlignment="Left" Margin="713,390,0,0" VerticalAlignment="Top" Height="18" Width="63" RenderTransformOrigin="0.305,2.184" Click="BtFirma_Click"/>

