<Window x:Class="WPFOtroInstrumento.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFOtroInstrumento"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="frame1" Content="Frame" Margin="146,0,0,0"/>
        <ComboBox x:Name="instrumentos" HorizontalAlignment="Left" Margin="10,143,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem>
                <TextBlock>Piano</TextBlock>
            </ComboBoxItem>
            <ComboBoxItem>
                <TextBlock>Guitarra</TextBlock>
            </ComboBoxItem>
        </ComboBox>
        <Button Content="Seleccionar" HorizontalAlignment="Left" Margin="10,234,0,0" VerticalAlignment="Top" Width="82" Click="Button_Click"/>
        <Button Content="Firma" HorizontalAlignment="Left" Margin="18,361,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</Window>
